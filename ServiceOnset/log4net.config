<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="System.Configuration.IgnoreSectionHandler" />
  </configSections>
  <log4net>
    <root>
      <appender-ref ref="Default" />
    </root>
    <appender name="Default" type="log4net.Appender.RollingFileAppender">
      <file value="logs/log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="yyyyMMdd&quot;.log&quot;" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <logger name="CMD">
      <appender-ref ref="CMD" />
    </logger>
    <appender name="CMD" type="log4net.Appender.RollingFileAppender">
      <file value="logs/CMD/log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="yyyyMMdd&quot;.log&quot;" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>
    <!--<logger name="{MyService}">
      <appender-ref ref="{MyService}" />
    </logger>
    <appender name="{MyService}" type="log4net.Appender.RollingFileAppender">
      <file value="logs/{MyService}/log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="yyyyMMdd&quot;.log&quot;" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>-->
  </log4net>
</configuration>