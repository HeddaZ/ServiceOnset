{
	"enableLog": true,
	"services": [
		{
			"name": "PingBaidu",
			"command": "ping",
			"arguments": "www.baidu.com",
			"workingDirectory": "",
			"runMode": "interval",
			"intervalInSeconds": 10,
			"useShellExecute": false,
			"allowWindow": false,
			"killExistingProcess": false,
			"enableLog": true
		},
        {
            "name": "mywin_AppPath",
            "command": "mywin",
            "killExistingProcess": true
        },
        {
			"name": "mywin_RelativePath",
            "command": "test\\mywin.exe",
            "runMode": "interval",
			"killExistingProcess": true
		}
	]
}